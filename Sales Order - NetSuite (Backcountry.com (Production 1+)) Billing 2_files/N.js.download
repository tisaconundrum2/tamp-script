        function cloneRecordValue(val)
        {
            var clone = val;
            if(util.isObject(val))
            {
                clone = recordUtil.clone({},val);
            }
            else if(util.isArray(val))
            {
                clone = val.map(function(entry){
                    return cloneRecordValue(entry);
                })
            }
            else if(util.isDate(val))
            {
                clone = new Date(val);
            }

            return clone;
        }

