function AVA_Ping()
{
	var Version = '';
	
	try
	{
		var response;
		AVA_InitSignatureObject(nlapiGetContext().getName());
		
		var ping = AvaTax.ping(AVA_AccountValue, nlapiDecrypt(AVA_LicenseKey, 'aes', Sha256.hash('AVATAX')));
		response = nlapiRequestURL(ping.url, ping.data, ping.headers, ping.method);
		if (response.getCode() == 200)
		{
			var responseBody = JSON.parse(response.getBody());
			Version = responseBody.version;
		}
	}
	catch(err)
	{
		nlapiLogExecution('Debug', 'Ping error', err.message);
	}
	
	return Version;
}

function AVA_Header(security)
{
	var soap = null;
	soap = '\t<soap:Header>\n';
	if(security != null)
	{
		soap += security;
	}
			soap += '\t\t<Profile xmlns="http://avatax.avalara.com/services">\n';
					soap += '\t\t\t<Name><![CDATA[' + nlapiGetContext().getName() + ']]></Name>\n';
					soap += '\t\t\t<Client><![CDATA[NetSuite OneWorld ' + nlapiGetContext().getVersion() + ' || ' + AVA_ClientAtt.substr(18) + 'v2; a0o0b000005sdCuAAI]]></Client>\n';
					soap += '\t\t\t<Adapter/>\n';
					soap += '\t\t\t<Machine/>\n';
			soap += '\t\t</Profile>\n';
	soap += '\t</soap:Header>\n';
	return soap;
}

function AVA_BuildEnvelope(actualcontents)
{
	var soap = null;
	soap = '<?xml version="1.0" encoding="utf-8"?>\n';
	soap += '<soap:Envelope xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/">\n';
		soap += actualcontents;
	soap += '</soap:Envelope>';
	return soap;
}

function AVA_CalculateTax()
{
	var AVA_DocType = AVA_RecordType();
	AVA_InitSignatureObject(nlapiGetContext().getName());
	Transaction = new AvaTax.transaction();
	AVA_GetTaxBody(AVA_DocType);
	var createoradjust = Transaction.createoradjust(AVA_AccountValue, nlapiDecrypt(AVA_LicenseKey, 'aes', Sha256.hash('AVATAX')));
	
	try
	{
		AVA_ConnectorEndTime = new Date();
		
		var StartTime = new Date();
		var response = nlapiRequestURL(createoradjust.url, createoradjust.data, createoradjust.headers, createoradjust.method);
		var EndTime = new Date();
		
		AVA_ConnectorStartTime = new Date();
		AVA_LatencyTime = EndTime.getTime() - StartTime.getTime();
		
		var resp = AVA_ReadResponse(response, AVA_DocType, StartTime);
		return resp;
	}
	catch(err)
	{
		if((AVA_EnableLogEntries == true || AVA_EnableLogEntries == 'T') && err.code != 'GetTaxError' && err.code != 'InvalidAddress' && err.code != 'ValueRequiredError')
		{
			AVA_Logs(AVA_LineCount, 'GetTax', nlapiGetRecordId(), '', '', 'CreateOrAdjustTransaction', 'Debug', 'Error', nlapiGetRecordType(), 'AVA_CalculateTax', err.message, err.code, 0);
		}
		
		if(err.code == 'SSS_CONNECTION_TIME_OUT' || err.code == 'SSS_REQUEST_TIME_EXCEEDED') // Logic for Retry of GetTax call - CLOUDERP-5326
		{
			nlapiLogExecution('DEBUG', 'Retry calling AvaTax', err.code);
			try
			{
				var StartTime = new Date();
				var response = nlapiRequestURL(createoradjust.url, createoradjust.data, createoradjust.headers, createoradjust.method);
				var EndTime = new Date();
				
				AVA_ConnectorStartTime = new Date();
				AVA_LatencyTime = EndTime.getTime() - StartTime.getTime();
				
				var resp = AVA_ReadResponse(response, AVA_DocType, StartTime);
				return resp;
			}
			catch(err)
			{
				if((AVA_EnableLogEntries == true || AVA_EnableLogEntries == 'T') && err.code != 'GetTaxError' && err.code != 'InvalidAddress')
				{
					AVA_Logs(AVA_LineCount, 'GetTax', nlapiGetRecordId(), '', '', 'CreateOrAdjustTransaction', 'Debug', 'Error', nlapiGetRecordType(), 'AVA_CalculateTax', 'After Retry - ' + err.message, err.code, 0);
				}
				
				if(err.code == 'GetTaxError' || err.code == 'InvalidAddress' || err.code == 'ValueRequiredError')
				{
					AVA_TotalTax = 0;
					
					if(nlapiGetFieldValue('custpage_ava_taxcodestatus') == 0 && (AVA_ShowMessages == 2 || AVA_ShowMessages == 3))
					{
						AVA_ErrorCode = err.message;
						alert("This Document has used AvaTax Services. " + err.message);
					}
					else
					{
						AVA_ErrorCode = err.message;
						nlapiLogExecution('DEBUG', 'Error Message', err.message);
					}
					
					AVA_SetDocTotal(AVA_DocType);
				}
				else
				{
					if(nlapiGetFieldValue('custpage_ava_taxcodestatus') == 0 && (AVA_ShowMessages == 2 || AVA_ShowMessages == 3))
					{
						alert("Please contact the administrator. " + err.message);
						AVA_ErrorCode = 'Please contact the Administrator. ' + err.message;
					}
					else
					{
						nlapiLogExecution('DEBUG', 'Please contact the administrator');
						nlapiLogExecution('DEBUG', 'Try/Catch Error', err.message);
						AVA_ErrorCode = err.message;
					}
				}
				return false;
			}
		}
		
		if(err.code == 'GetTaxError' || err.code == 'InvalidAddress' || err.code == 'ValueRequiredError')
		{
			AVA_TotalTax = 0;
			
			if(nlapiGetFieldValue('custpage_ava_taxcodestatus') == 0 && (AVA_ShowMessages == 2 || AVA_ShowMessages == 3))
			{
				AVA_ErrorCode = err.message;
				alert("This Document has used AvaTax Services. " + err.message);
			}
			else
			{
				AVA_ErrorCode = err.message;
				nlapiLogExecution('DEBUG', 'Error Message', err.message);
			}
			
			AVA_SetDocTotal(AVA_DocType);
		}
		else
		{
			if(nlapiGetFieldValue('custpage_ava_taxcodestatus') == 0 && (AVA_ShowMessages == 2 || AVA_ShowMessages == 3))
			{
				alert("Please contact the administrator. " + err.message);
				AVA_ErrorCode = 'Please contact the Administrator. ' + err.message;
			}
			else
			{
				nlapiLogExecution('DEBUG', 'Please contact the administrator');
				nlapiLogExecution('DEBUG', 'Try/Catch Error', err.message);
				AVA_ErrorCode = err.message;
			}
		}
		return false;
	}
	
	AVA_LogTaxResponse('T', response, StartTime);
	return true;
}

