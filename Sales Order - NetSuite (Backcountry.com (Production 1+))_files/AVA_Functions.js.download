function AVA_VerifyCredentials()
{ 
	if(nlapiGetContext().getExecutionContext() == 'webstore' && nlapiGetFieldValue('entity') == 0)
	{
		return 1;
	}
	var searchresult = nlapiSearchRecord('customrecord_avaconfig', null, null, null);
	if(searchresult != null)
	{
		for(var i=0; searchresult != null && i < searchresult.length; i++)
		{
			var record = nlapiLoadRecord('customrecord_avaconfig', searchresult[i].getId());
			nlapiSetFieldValue('custpage_ava_readconfig', JSON.stringify(record));
			var AVA_LoadValues = AVA_LoadValuesToGlobals(record);
			break;
		}
		return 0;
	}
	else
	{
		return 1;
	}
}

function AVA_CalculateOnDemand()
{
	AVA_LineCount = 0;
	AVA_ResultCode = '';
	var ConnectorStartTime = new Date();
	nlapiSetFieldValue('custpage_ava_taxcodestatus', '0');
	nlapiSetFieldValue('custpage_ava_notemsg', '');

	try
	{
		if(nlapiGetFieldValue('custbody_ava_taxoverride') == 'T' || nlapiGetFieldValue('custpage_ava_postingperiod') == 'T')
		{
			return true;
		}
		
		if(AVA_EntityUseCode == 'T' || AVA_EntityUseCode == true)
		{
			nlapiSetFieldValue('custpage_ava_usecodeusuage', 'T');
		}
		
		if((AVA_TaxInclude == 'T' || AVA_TaxInclude == true) && nlapiGetFieldValue('custbody_ava_taxinclude') != null && nlapiGetFieldValue('custbody_ava_taxinclude') == 'T')
		{
			var DiscountRate = nlapiGetFieldValue('discountrate');
			
			if(DiscountRate != null && DiscountRate.indexOf('%') != -1)
			{
				if(nlapiGetFieldValue('custbody_ava_discountamount') == null || nlapiGetFieldValue('custbody_ava_discountamount').length <= 0)
				{
					nlapiSetFieldValue('custbody_ava_discountamount', nlapiGetFieldValue('discounttotal')); // Store original discount amount
				}
			}
		}
		
		if((nlapiGetFieldValue('custpage_ava_rectype') == 'creditmemo' || nlapiGetFieldValue('custpage_ava_rectype') == 'cashrefund' || nlapiGetFieldValue('custpage_ava_rectype') == 'returnauthorization') && nlapiGetFieldValue('createdfrom') != null && nlapiGetFieldValue('createdfrom').length > 0 && (AVA_UseInvoiceAddress == 'T' || AVA_UseInvoiceAddress == true))
		{
			if(nlapiGetFieldValue('custpage_ava_rectype') == 'creditmemo' && nlapiGetFieldValue('custpage_ava_createdfromrecordid') != null && nlapiGetFieldValue('custpage_ava_createdfromrecordid').length > 0)
			{
