function LTrim(str)
{
	for (var i=0; ((str.charAt(i)<=" ")&&(str.charAt(i)!="")); i++);
	return str.substring(i,str.length);
}

function RTrim(str)
{
	for (var i=str.length-1; ((str.charAt(i)<=" ")&&(str.charAt(i)!="")); i--);
	return str.substring(0,i+1);
}

function Trim(str)
{
	return LTrim(RTrim(str));
}

function AVA_ViewTaxDetails(request, response)
{
	if(AVA_CheckService('TaxSvc') == 0 && AVA_CheckSecurity( 6 ) == 0)
	{
		if(request.getMethod() == 'GET')
		{
			var AVA_GetTaxHistoryForm = nlapiCreateForm('AvaTax Transaction Details');
			AVA_GetTaxHistoryForm.setTitle('AvaTax Transaction Details');
			
			AVA_ReadConfig('0'); 
			AVA_InitSignatureObject(nlapiGetContext().getName());
			var DefCompanyCode = AVA_GetDefaultCompanyCode(request.getParameter('subid'));
			DefCompanyCode = ((DefCompanyCode != null && DefCompanyCode.length > 0) ? DefCompanyCode : request.getParameter('subid'));
			var getTaxHistory = AvaTax.getTaxHistory(AVA_AccountValue, nlapiDecrypt(AVA_LicenseKey, 'aes', Sha256.hash('AVATAX')), request.getParameter('doctype'), request.getParameter('doccode'), DefCompanyCode);

			var AVA_GetHistoryResponse = nlapiRequestURL(getTaxHistory.url, getTaxHistory.data, getTaxHistory.headers, getTaxHistory.method);
			if (AVA_GetHistoryResponse.getCode() == 200)
			{
				var responseBody = JSON.parse(AVA_GetHistoryResponse.getBody());
				
				AVA_GetTaxHistoryForm.addField('ava_nstransno', 'text',    'NetSuite Transaction No');
				AVA_GetTaxHistoryForm.getField('ava_nstransno').setDefaultValue(request.getParameter('ns_transid'));
				AVA_GetTaxHistoryForm.getField('ava_nstransno').setDisplayType('inline');
				
				AVA_GetTaxHistoryForm.addField('ava_docno',     'text',   'AvaTax Document No');
				AVA_GetTaxHistoryForm.getField('ava_docno').setDefaultValue(responseBody.code);
				AVA_GetTaxHistoryForm.getField('ava_docno').setDisplayType('inline');
				
				AVA_GetTaxHistoryForm.addField('ava_customer',    'text',   'Customer');
				AVA_GetTaxHistoryForm.getField('ava_customer').setDefaultValue(responseBody.customerVendorCode);
				AVA_GetTaxHistoryForm.getField('ava_customer').setDisplayType('inline');
				
				AVA_GetTaxHistoryForm.addField('ava_docdate',   'text',   'Document Date');
				AVA_GetTaxHistoryForm.getField('ava_docdate').setDefaultValue(AVA_DateFormat(nlapiGetContext().getSetting('PREFERENCE', 'DATEFORMAT'), responseBody.date));
				AVA_GetTaxHistoryForm.getField('ava_docdate').setDisplayType('inline');
				
				AVA_GetTaxHistoryForm.addField('ava_docstatus',   'text',   'Document Status');
				AVA_GetTaxHistoryForm.getField('ava_docstatus').setDefaultValue(responseBody.status);
				AVA_GetTaxHistoryForm.getField('ava_docstatus').setDisplayType('inline');
				
				AVA_GetTaxHistoryForm.addField('ava_calcdate',    'text',   'Tax Calculation Date');
				AVA_GetTaxHistoryForm.getField('ava_calcdate').setDefaultValue(AVA_DateFormat(nlapiGetContext().getSetting('PREFERENCE', 'DATEFORMAT'),responseBody.taxDate));
				AVA_GetTaxHistoryForm.getField('ava_calcdate').setDisplayType('inline');
				
				AVA_GetTaxHistoryForm.addField('ava_doctype',   'text',   'Document Type');
				AVA_GetTaxHistoryForm.getField('ava_doctype').setDefaultValue(responseBody.type);
				AVA_GetTaxHistoryForm.getField('ava_doctype').setDisplayType('inline');
				
				AVA_GetTaxHistoryForm.addTab('custpage_avatab', 'AvaTax Transaction Details');
				AVA_GetTaxHistoryForm = AVA_GetHistoryTab( AVA_GetTaxHistoryForm, responseBody, 'custpage_avatab');
				
				response.writePage(AVA_GetTaxHistoryForm);
			}
			else
			{
				var AVA_Message;
				var errorBody = JSON.parse(AVA_GetHistoryResponse.getBody())
				var errorDetails = errorBody.error.details;
				
				for(var i = 0; errorDetails != null && i < errorDetails.length; i++)
				{
					AVA_Message = errorDetails[i].message;
					break;
				}
				
				var AVA_Notice = AVA_NoticePage(AVA_Message);
				response.write(AVA_Notice);
			}     
		}
	}
}

