//Function to generate Logs
function AVA_Logs(LineCount, EventBlock, DocCode, ConnectorTime, ConnectorLatency, Operation, LogType, LogLevel, Source, FunctionName, Message, StackTrace, ConfigFlag)
{
	var AVA_LogArray = {};
	
	AVA_LogArray.CallerAccuNum 	  = AVA_AccountValue;
	AVA_LogArray.LogType 		  = LogType;
	AVA_LogArray.LogLevel 		  = LogLevel;
	AVA_LogArray.ConnectorName 	  = 'NetSuite OneWorld ' + nlapiGetContext().getVersion() + ' || ' + AVA_ClientAtt.substr(18) + 'v2; a0o0b000005sdCuAAI';
	AVA_LogArray.ConnectorVersion = AVA_ClientAtt;
	AVA_LogArray.ERPName		  = 'NetSuite OneWorld';
	AVA_LogArray.ERPversion		  = nlapiGetContext().getVersion();
	AVA_LogArray.Operation 		  = Operation;
	AVA_LogArray.ServiceURL 	  = (AVA_ServiceUrl == '0' ? 'Production' : 'Sandbox');
	AVA_LogArray.Source 		  = Source;
	
	if(ConfigFlag == 0)
	{
		AVA_LogArray.DocCode 		  = ((DocCode != null && DocCode.length > 0) ? DocCode : '');
		AVA_LogArray.LineCount 		  = LineCount;
		AVA_LogArray.EventBlock 	  = EventBlock;
		AVA_LogArray.ConnectorTime 	  = ConnectorTime;
		AVA_LogArray.ConnectorLatency = ConnectorLatency;
	}
	
	AVA_LogArray.FunctionName 	  = FunctionName;
	AVA_LogArray.Message 		  = Message;
	
	if(LogLevel == 'Exception')
	{
		AVA_LogArray.StackTrace   = StackTrace;
	}
	
	var LogDetails = JSON.stringify(AVA_LogArray);
	var LogDetails = LogDetails.substring(1, LogDetails.length - 1); // Remove curly braces from LogDetails
	
	var soapLogHead = {};
	soapLogHead['Content-Type']  ='text/xml';
	
	nlapiRequestURL('https://webhook.logentries.com/noformat/logs/' + AVA_LogEntryToken, LogDetails, soapLogHead);
}

